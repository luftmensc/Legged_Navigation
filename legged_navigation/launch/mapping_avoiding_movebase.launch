<launch>


    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find legged_navigation)/config/costmap_common_params2.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find legged_navigation)/config/costmap_common_params2.yaml" command="load" ns="local_costmap" />

        <rosparam file="$(find legged_navigation)/config/global_costmap_params2.yaml" command="load" />
        <rosparam file="$(find legged_navigation)/config/local_costmap_params2.yaml" command="load" />

        <rosparam file="$(find legged_navigation)/config/base_local_planner_params2.yaml" command="load" />
        <rosparam file="$(find legged_navigation)/config/move_base_params2.yaml" command="load" />

        <param name="base_global_planner" type="string" value="navfn/NavfnROS" />
        <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>


        <remap from="odom" to="odom" />
    </node>
</launch>
